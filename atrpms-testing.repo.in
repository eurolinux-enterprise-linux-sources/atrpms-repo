[atrpms-testing]
name=@DISTFULLNAME@ $releasever - $basearch - ATrpms testing
failovermethod=priority
baseurl=http://dl.atrpms.net/@DISTINITIALS@$releasever-$basearch/atrpms/testing
enabled=0
gpgcheck=1
gpgkey=file:///etc/pki/rpm-gpg/RPM-GPG-KEY-atrpms

[atrpms-testing-debuginfo]
name=@DISTFULLNAME@ $releasever - $basearch - ATrpms testing - Debug
failovermethod=priority
baseurl=http://dl.atrpms.net/debug/@DISTINITIALS@$releasever-$basearch/atrpms/testing
enabled=0
gpgcheck=1
gpgkey=file:///etc/pki/rpm-gpg/RPM-GPG-KEY-atrpms

[atrpms-testing-source]
name=@DISTFULLNAME@ $releasever - $basearch - ATrpms testing - Source
failovermethod=priority
baseurl=http://dl.atrpms.net/src/@DISTINITIALS@$releasever-$basearch/atrpms/testing
enabled=0
gpgcheck=1
gpgkey=file:///etc/pki/rpm-gpg/RPM-GPG-KEY-atrpms
