[atrpms-bleeding]
name=@DISTFULLNAME@ $releasever - $basearch - ATrpms bleeding
failovermethod=priority
baseurl=http://dl.atrpms.net/@DISTINITIALS@$releasever-$basearch/atrpms/bleeding
enabled=0
gpgcheck=1
gpgkey=file:///etc/pki/rpm-gpg/RPM-GPG-KEY-atrpms

